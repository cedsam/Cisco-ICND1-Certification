
!
version 12.4
service timestamps debug datetime msec
service timestamps log datetime msec
service password-encryption
!
hostname R3
!
boot-start-marker
boot-end-marker
!
enable secret 5
!
no aaa new-model
!
resource policy
!
no network-clock-participate slot 1 
no network-clock-participate wic 0 
ip subnet-zero
ip cef
!
!
no ip dhcp use vrf connected
!
!
no ip ips deny-action ips-interface
ip domain name samardzija.com
ip ssh version 2
!
!
!
!
username samardzija secret 5
!
! 
!
!
!
interface Loopback3
 ip address 3.3.3.3 255.255.255.255
!
interface FastEthernet0/0
 description R3->SW3
 ip address 192.168.1.94 255.255.255.224
 speed 100
 full-duplex
 no cdp enable
!
interface Serial0/0
 description R3->R1
 ip address 192.168.1.157 255.255.255.224
 no fair-queue
 clockrate 2000000
!
interface ATM0/1
 no ip address
 shutdown
 no atm ilmi-keepalive
 dsl operating-mode auto 
!
router ospf 1
 router-id 3.3.3.3
 log-adjacency-changes
 passive-interface default
 no passive-interface Serial0/0
 network 192.168.1.64 0.0.0.0 area 0
 network 192.168.1.64 0.0.0.31 area 0
 network 192.168.1.157 0.0.0.0 area 0
!
ip classless
!
no ip http server
no ip http secure-server
!
!
!
control-plane
!
!
!
!
line con 0
 exec-timeout 0 0
 logging synchronous
 login local
line aux 0
line vty 0 1
 exec-timeout 0 0
 logging synchronous
 login local
 transport input ssh
line vty 2 4
 exec-timeout 0 0
 no login
 transport input none
line vty 5 181
 exec-timeout 0 0
 no login
 transport input none
!
!
end
